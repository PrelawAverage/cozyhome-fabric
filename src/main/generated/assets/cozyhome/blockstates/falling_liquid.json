{
  "multipart": [
    {
      "apply": {
        "model": "cozyhome:block/stream_water_falling"
      },
      "when": {
        "contains": "water"
      }
    },
    {
      "apply": {
        "model": "cozyhome:block/stream_lava_falling"
      },
      "when": {
        "contains": "lava"
      }
    },
    {
      "apply": {
        "model": "cozyhome:block/stream_water_landing"
      },
      "when": {
        "contains": "water",
        "has_under": "true"
      }
    },
    {
      "apply": {
        "model": "cozyhome:block/stream_lava_landing"
      },
      "when": {
        "contains": "lava",
        "has_under": "true"
      }
    }
  ]
}